  <persistence-unit name="webProject" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>domain.Admor</class>
    <class>domain.Anclado</class>
    <class>domain.Comentario</class>
    <class>domain.Comun</class>
    <class>domain.Estado</class>
    <class>domain.Municipio</class>
    <class>domain.Normal</class>
    <class>domain.Post</class>
